@model MySolution.Models.System.Users.RegisterRequest

<!DOCTYPE html>
<html lang="en">
<head>
	<title>Login V15</title>
	<meta charset="Đăng ký tài khoản">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<!--===============================================================================================-->
	<link rel="icon" type="/image/png" href="images/icons/favicon.ico" />
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/vendor/bootstrap/css/bootstrap.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/fonts/Linearicons-Free-v1.0.0/icon-font.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/vendor/animate/animate.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/vendor/css-hamburgers/hamburgers.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/vendor/animsition/css/animsition.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/vendor/select2/select2.min.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/vendor/daterangepicker/daterangepicker.css">
	<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/css/util.css">
	<link rel="stylesheet" type="text/css" href="/css/main.css">
	<!--===============================================================================================-->
</head>
<body>

	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100">
				<div class="login100-form-title" style="background-image: url(/img/hero/banner.jpg);">
					<span class="login100-form-title-1">
						Đăng Ký
					</span>
				</div>

				<form class="login100-form validate-form" asp-controller="Account" asp-action="Register" method="post">
					<div asp-validation-summary="ModelOnly" class="text-danger"></div>
					<div class="wrap-input100 validate-input m-b-26">
						<span class="label-input100">Tên</span>
						<input asp-for="FirstName" placeholder="Nhập tên">
					</div>
					<div class="wrap-input100 validate-input m-b-26">
						<span class="label-input100">Họ</span>
						<input asp-for="LastName" placeholder="Nhập họ">
					</div>
					<div class="wrap-input100 validate-input m-b-26">
						<span class="label-input100">Tên tài khoản</span>
						<input asp-for="UserName" placeholder="Nhập tên tài khoản">
					</div>
					<div class="wrap-input100 validate-input m-b-26">
						<span class="label-input100">Email</span>
						<input asp-for="Email" placeholder="Nhập email">
					</div>
					<div class="wrap-input100 validate-input m-b-26">
						<span class="label-input100">Ngày sinh</span>
						<input asp-for="Dob">
					</div>
					<div class="wrap-input100 validate-input m-b-26">
						<span class="label-input100">Số điện thoại</span>
						<input asp-for="PhoneNumber" placeholder="Nhập Số điện thoại">
					</div>
					<div class="wrap-input100 validate-input m-b-18">
						<span class="label-input100">Password</span>
						<input class="input100" type="password" asp-for="Password" placeholder="Nhập mật khẩu">
					</div>

					<div class="wrap-input100 validate-input m-b-18">
						<span class="label-input100">xác nhận mật khẩu</span>
						<input class="input100" type="password" asp-for="ConfirmPassWord" placeholder="Nhập lại mật khẩu">
					</div>

					<div class="container-login100-form-btn">
						<button class="login100-form-btn">
							Đăng ký
						</button>
					</div>
				</form>
			</div>
		</div>
	</div>

	<!--===============================================================================================-->
	<script src="/vendor/jquery/jquery-3.2.1.min.js"></script>
	<!--===============================================================================================-->
	<script src="/vendor/animsition/js/animsition.min.js"></script>
	<!--===============================================================================================-->
	<script src="/vendor/bootstrap/js/popper.js"></script>
	<script src="/vendor/bootstrap/js/bootstrap.min.js"></script>
	<!--===============================================================================================-->
	<script src="/vendor/select2/select2.min.js"></script>
	<!--===============================================================================================-->
	<script src="/vendor/daterangepicker/moment.min.js"></script>
	<script src="/vendor/daterangepicker/daterangepicker.js"></script>
	<!--===============================================================================================-->
	<script src="/vendor/countdowntime/countdowntime.js"></script>
	<!--===============================================================================================-->
	<script src="/js/main-lg.js"></script>

</body>
</html>